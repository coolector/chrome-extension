{{#each plugins}}
<div class="coolector-box" {{#unless results.length}}style="overflow: hidden;"{{/unless }}>
    <div class="coolector-box-title">
        {{name}} v{{version}}
        <a target="_blank" href="{{url}}">{{> icon-open-white }}</a>
    </div>
    {{#if results.length}}
    <div class="coolector-box-content">
        <div class="coolector-box-content-title">
            Item saved.
            {{> icon-more }}
            <div class="coolector-box-title-submenu" id="coolector-menu">
                <div class="coolector-box-title-submenu-button rttbtn1">{{> icon-delete }} Remove Item</div>
                <div class="coolector-box-title-submenu-button rttbtn2"><a target="_blank" href="{{url}}">{{> icon-open }} Open Coolector</a></div>
            </div>
        </div>
        <div class="coolector-box-content-tags">
            {{> icon-tag }}
            <input type="text" id="coolector-tags" class="coolector-box-content-tags-input" placeholder="Add tags" />
        </div>
        <img src="images/girl.jpg" class="coolector-box-content-image" />
        <div class="coolector-box-content-text">
            Some title that can extend on multiple pages if we need and want.
        </div>
    </div>
    {{/if}}
</div>
{{/each}}